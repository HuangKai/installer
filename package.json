{
  "name": "figma-plugins-desktop-injector",
  "productName": "Figma Plugins Desktop Injector",
  "version": "1.4.4",
  "description": "Injects Figma plugins manager to Figma Desktop.",
  "repository": "https://github.com/cdes/figma-plugins-desktop-injector",
  "author": "Ahmed Al Haddad <haddad@ah.sa>",
  "main": "./index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron ./",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.haddad.figments",
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": "zip"
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "asar": "^0.14.5",
    "electron-debug": "^2.0.0",
    "electron-is-dev": "^1.0.1",
    "electron-prompt": "^1.2.0",
    "electron-store": "^2.0.0",
    "electron-unhandled": "^2.1.0",
    "electron-util": "^0.10.2",
    "fs-extra": "^7.0.1",
    "inject-code": "^1.0.0",
    "jquery": "^3.3.1",
    "menubar": "^5.2.3",
    "ps-list": "^6.0.0",
    "tree-kill": "^1.2.1"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0"
  }
}